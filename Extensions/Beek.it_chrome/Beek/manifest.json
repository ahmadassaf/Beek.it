{
  "name": "Beek.it",
  "version": "0.1",
  "manifest_version": 2,
  "description": "A Smart Semantic Bookmark Organizer",
  "homepage_url": "http://beek.it",
  "icons": {
    "16": "icons/beek-16.png",
    "48": "icons/beek-48.png",
    "128": "icons/beek-128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/beek-19.png"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
    "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["css/beek.css"],
      "js": [
        "lib/jquery.js",
        "js/beek.js"
        ],
       "run_at" : "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "http://*/",
    "https://*/"
  ],
  "offline_enabled": true,
  "web_accessible_resources": [
    "css/beek.css",
    "lib/underscore.js",
    "icons/beek.png"
  ]
}